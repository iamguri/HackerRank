#include <bits/stdc++.h>

using namespace std;

int main() {
   
  int a[100],b[100],i,alicepoint=0,bobpoint=0;

for(i=0;i<3;i++){
    cin>>a[i];
}
for(i=0;i<3;i++){
    cin>>b[i];
}
for(i=0;i<3;i++){
    if(a[i]>b[i]){
        alicepoint++;
    }
    else if(a[i]<b[i]){
        bobpoint++;
    }
}
cout<<alicepoint<<" "<<bobpoint; 
return 0;
}
