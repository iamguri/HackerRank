#include <bits/stdc++.h>

using namespace std;

int main(){

    int a[1000],i,n;
   float  pos=0,neg=0,zer=0;
    cin>>n;

    for(i=0;i<n;i++)
        {
            cin>>a[i];
        if(a[i]>0)
            pos++;
        else if(a[i]<0)
            neg++;    
        else if (a[i]==0) 
            zer++;
    }
cout<<setprecision(6)<<pos/n<<endl<<setprecision(6)<<neg/n<<endl<<setprecision(6)<<zer/n;
    return 0;

}
